<Application x:Class="WorkOrder.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:local="clr-namespace:WorkOrder"
              xmlns:vm="clr-namespace:WorkOrder.ViewModel"
               ShutdownMode="OnExplicitShutdown" StartupUri="MainWindow.xaml" 
              >
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="locator" />
            <FontFamily x:Key="IconFont">pack://application:,,,/WorkOrder;component/Assets/Fonts/#fontawesome</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
